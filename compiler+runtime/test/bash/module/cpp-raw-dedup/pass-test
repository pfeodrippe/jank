#!/usr/bin/env bash

set -euo pipefail

# Reproduces issue #582: cpp/raw functions should not cause ODR violations
# during AOT compilation. This test tries to compile a module with inline
# C++ functions defined via cpp/raw.

cd "$(dirname "$0")"

# Test compile-module command
echo "Testing: jank --module-path src compile-module issue-582"
jank --module-path src compile-module issue-582

echo "âœ“ Test passed: compile-module succeeded"
